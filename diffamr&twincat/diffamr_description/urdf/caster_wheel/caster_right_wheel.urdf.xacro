<?xml version="1.0"?>

<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="caster_right_wheel">
    <xacro:macro name="caster_right_wheel" params="prefix reflect reflect_x">
        <link name="${prefix}_caster_right_wheel">
            <visual>
                <origin rpy="0 0 1.57" xyz="0 0 0"/>
                <geometry>
                    <mesh filename="package://diffamr_description/meshes/caster_wheel/caster_right_wheel.stl" scale="0.001 0.001 0.001"/>
                </geometry>
                <material name="red"/>
            </visual>
            <collision>
