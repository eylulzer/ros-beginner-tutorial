<?xml version="1.0" encoding="UTF-8"?>
<launch>

    <node name="diffamr_hardware_interface_node" pkg="diffamr_base" type="diffamr_hardware_interface" respawn="false"
          output="screen" required="true"/>  <!--1-->

    <rosparam file="$(find diffamr_base)/config/control.yaml" command="load"/> <!--2-->

    <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
          output="screen" args="diffamr_joint_state_controller diffamr_diff_drive_controller " required="true"/> <!--3-->
      
</launch>
