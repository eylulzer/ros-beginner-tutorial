<?xml version="1.0"?>
<robot name="myfirst">
  <link name="base_link">
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </link>

  <link name="right_wheel">
    <visual>
      <geometry>
        <cylinder length="0.02" radius="0.08"/>
      </geometry>
      <origin rpy="1.5707 0 0" xyz="0 0.2335 0"/>
    </visual>
  </link>

  <joint name="base_to_right_wheel" type="fixed">
    <parent link="base_link"/>
    <child link="right_wheel"/>
    <origin xyz="0 0 0"/>
  </joint>


  <link name="left_wheel">
    <visual>
      <geometry>
        <cylinder length="0.02" radius="0.08"/>
      </geometry>
      <origin rpy="1.5707 0 0" xyz="0 -0.2335 0"/>
    </visual>
  </link>

  <joint name="base_to_left_wheel" type="fixed">
    <parent link="base_link"/>
    <child link="left_wheel"/>
    <origin xyz="0 0 0"/>
  </joint>


  <link name="right_front_caster_wheel">
    <visual>
      <geometry>
        <cylinder length="0.03" radius="0.04"/>
      </geometry>
      <origin rpy="1.5707 0 0" xyz="-0.375 0.170 0"/>
    </visual>
  </link>

  <joint name="base_to_right_front_caster_wheel" type="fixed">
    <parent link="base_link"/>
    <child link="right_front_caster_wheel"/>
    <origin xyz="0 0 0"/>
  </joint>


  <link name="left_front_caster_wheel">
    <visual>
      <geometry>
        <cylinder length="0.03" radius="0.04"/>
      </geometry>
      <origin rpy="1.5707 0 0" xyz="-0.375  -0.170 0"/>
    </visual>
  </link>

  <joint name="base_to_left_front_caster_wheel" type="fixed">
    <parent link="base_link"/>
    <child link="left_front_caster_wheel"/>
    <origin xyz="0 0 0"/>
  </joint>


  <link name="right_back_caster_wheel">
    <visual>
      <geometry>
        <cylinder length="0.03" radius="0.04"/>
      </geometry>
      <origin rpy="1.5707 0 0" xyz="0.375  0.170 0"/>
    </visual>
  </link>

  <joint name="base_to_right_back_caster_wheel" type="fixed">
    <parent link="base_link"/>
    <child link="right_back_caster_wheel"/>
    <origin xyz="0 0 0"/>
  </joint>


  <link name="left_back_caster_wheel">
    <visual>
      <geometry>
        <cylinder length="0.03" radius="0.04"/>
      </geometry>
      <origin rpy="1.5707 0 0" xyz="0.375  -0.170 0"/>
    </visual>
  </link>

  <joint name="base_to_left_back_caster_wheel" type="fixed">
    <parent link="base_link"/>
    <child link="left_back_caster_wheel"/>
    <origin xyz="0 0 0"/>
  </joint>  

</robot>

